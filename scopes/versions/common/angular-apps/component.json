{
  "componentId": {
    "name": "versions/common/angular-apps",
    "scope": "teambit.angular"
  },
  "propagate": true,
  "extensions": {
    "teambit.pkg/pkg": {
      "packageManagerPublishArgs": [
        "--access public"
      ],
      "packageJson": {
        "name": "@teambit/angular-apps",
        "private": false,
        "engines": {
          "node": ">=12.15.0"
        }
      }
    },
    "teambit.dependencies/dependency-resolver": {
      "policy": {
        "dependencies": {
          "@angular-devkit/build-angular": "-",
          "typescript": "-"
        },
        "peerDependencies": {
          "@angular-devkit/build-angular": ">= 0.0.1",
          "typescript": ">= 3.5.3"
        }
      }
    }
  }
}
