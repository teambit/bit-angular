{
  "componentId": {
    "scope": "teambit.angular",
    "name": "dev-services/compiler/ng-packagr"
  },
  "propagate": true,
  "extensions": {
    "teambit.pkg/pkg": {
      "packageManagerPublishArgs": [
        "--access public"
      ],
      "packageJson": {
        "name": "@teambit/ng-packagr",
        "private": false,
        "engines": {
          "node": ">=12.15.0"
        }
      }
    },
    "teambit.dependencies/dependency-resolver": {
      "policy": {
        "dependencies": {
          "@angular/compiler-cli": "-",
          "chalk": "^4.1.2",
          "ng-packagr": "-"
        },
        "peerDependencies": {
          "@angular/compiler-cli": ">= 8.0.0",
          "ng-packagr": ">=5.0.0"
        },
        "devDependencies": {
          "@babel/runtime": "7.22.15"
        }
      }
    }
  }
}
