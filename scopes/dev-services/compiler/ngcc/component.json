{
  "componentId": {
    "name": "dev-services/compiler/ngcc"
  },
  "propagate": true,
  "extensions": {
    "teambit.pkg/pkg": {
      "packageManagerPublishArgs": [
        "--access public"
      ],
      "packageJson": {
        "name": "@teambit/ngcc",
        "private": false,
        "engines": {
          "node": ">=12.15.0"
        }
      }
    },
    "teambit.dependencies/dependency-resolver": {
      "policy": {
        "dependencies": {
          "@angular/compiler-cli": "-",
          "@types/fs-extra": "~9.0.7",
          "fs-extra": "9.1.0"
        },
        "peerDependencies": {
          "@angular/compiler-cli": ">= 8.0.0"
        }
      }
    }
  }
}
