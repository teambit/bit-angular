          "@typescript-eslint/parser": "^5.29.0",
          "eslint": "^8.18.0",
          "events": "^3.2.0",
          "html-loader": "~2.1.2",
          "jest": "^28.0.0",
          "jest-preset-angular": "~12.1.0",
          "ng-packagr": "~14.0.2",
          "react-dev-utils": "10.2.1",
          "remark": "~13.0.0",
          "remark-frontmatter": "~3.0.0",
          "remark-html": "13.0.1",
          "remark-loader": "~4.0.0",
          "remark-prism": "~1.3.6",
          "rxjs": "~7.4.0",
          "tslib": "^2.3.0",
          "ts-node": "^9.1.1",
          "typescript": "~4.7.2",
