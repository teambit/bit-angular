{
  "componentId": {
    "name": "dev-services/compiler/angular-elements"
  },
  "propagate": true,
  "extensions": {
    "teambit.pkg/pkg": {
      "packageManagerPublishArgs": [
        "--access public"
      ],
      "packageJson": {
        "name": "@teambit/angular-elements",
        "private": false,
        "engines": {
          "node": ">=12.15.0"
        }
      }
    },
    "teambit.dependencies/dependency-resolver": {
      "policy": {
        "dependencies": {
          "@babel/core": "7.16.5",
          "@rollup/plugin-babel": "^5.3.0",
          "@rollup/plugin-html": "^0.2.4",
          "@rollup/plugin-json": "^4.1.0",
          "@rollup/plugin-node-resolve": "^13.0.4",
          "@teambit/react.ui.docs-app": "^0.0.173",
          "@types/cacache": "^15.0.1",
          "cacache": "^15.3.0",
          "esbuild": "^0.14.9",
          "rollup": "~2.58.0",
          "terser": "^5.8.0"
        },
        "devDependencies": {
          "@teambit/harmony": "^0.2.11"
        },
        "peerDependencies": {
          "@angular-devkit/build-optimizer": ">= 0.0.1",
          "@angular/compiler-cli": ">= 8.0.0",
          "@angular/core": ">= 8.0.0",
          "@angular/elements": ">= 8.0.0",
          "@angular/platform-browser": ">= 8.0.0",
          "@angular/platform-browser-dynamic": ">= 8.0.0"
        }
      }
    }
  }
}
